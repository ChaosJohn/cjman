# export CJMAN_HOME="${HOME}/.cjman"

export BASE_URL="https://raw.githubusercontent.com/ChaosJohn/cjman/master"
if grep -qF "gitcode.com" "${CJMAN_HOME}/.git/config"; then
  export BASE_URL="https://raw.gitcode.com/ChaosJohn/cjman/raw/master"
fi

export VERSIONS_URL="${BASE_URL}/versions.txt"

function error() {
  tput sgr0
  tput setaf 1
  echo "ERR: $1"
  tput sgr0
  # exit 1
}